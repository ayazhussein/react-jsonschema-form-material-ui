{
  "compact": true,
  "presets": [
      [
          "@babel/preset-env",
          {
              "modules": false
          }
      ],
      "@babel/preset-react",
      "@babel/preset-flow"
  ],
  "plugins": [
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-proposal-class-properties"
  ],
  "env": {
      "test": {
          "plugins": ["@babel/plugin-transform-modules-commonjs"]
      }
  }
}
